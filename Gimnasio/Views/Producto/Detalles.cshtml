@model List<Gimnasio.Models.Producto>

@{
    ViewData["Title"] = "Detalles de productos";
}


<h1 class="text-center">Detalles de los productos</h1>
<table class="table table-striped" id="formatDataTable">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
            <th>Stock</th>
            <th>Precio</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Nombre</td>
                <td>@item.Stock</td>
                <td>@item.Precio</td>
            </tr>
        }
    </tbody>
</table>


<script src="~/js/DataTableJS.js"></script>