@model Contrato
@if (Model == null)
{
    <div class="alert alert-warning" role="alert">
        El usuario no tiene contrato en el gimnasio.
    </div>
}
else
{
    <div class="alert alert-success" role="alert">
        <ul>
            <li>Fecha de inicio: @Model.FechaInicio</li>
            <li>Fecha de fin: @Model.FechaFin</li>
            <li>Método de pago: @Model.FormaPago</li>
        </ul>
    </div>
}